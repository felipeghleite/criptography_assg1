#include <stdio.h>

void main(){
	unsigned short ch;
	FILE *fpIn, *fpOut;
	int i = 0;
	unsigned int keyLength;
	unsigned char initCh;

	fpOut = fopen("freq.txt", "w");
	
	for(keyLength = 2; keyLength < 10 ; keyLength++){

        	fpIn = fopen("ctext.txt", "r");
		fscanf(fpIn,"%02X", &initCh);
		fseek(fpIn, keyLength, SEEK_CUR);
		
		while(fscanf(fpIn, "%02X", &ch) != EOF){
		
			if(initCh == ch)
			{
				i++;
				printf("%02X", ch);
				printf("\n");
			}
		
			fseek(fpIn, keyLength, SEEK_CUR);
		}

	fclose(fpIn);	
	
	}

	fclose(fpIn);
	fclose(fpOut);
	return;
}


